alias -s gz='tar -xzvf'
alias -s bz2='tar -xjvf'
alias -g L='| less'

alias ls='ls -GF'
alias grep='grep --color=auto'
alias mv='mv -i'
alias rm='rm -I'
alias du='du -h'
alias cd.dot='cd ~/.dotfiles && git status'
alias sprunge="curl -F 'sprunge=<-' http://sprunge.us"
alias myip="curl ifconfig.me/ip"
alias nohistory="unset HISTFILE"
