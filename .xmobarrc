Config { font = "-*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*"
       , bgColor = "#161616"
       , fgColor = "#4d4d4d"
       , position = Top
	   , lowerOnStart = True
       , commands = [ Run Network "eth0" ["-L","0","-H","32","--normal","#429942","--high","#A36666"] 50
					, Run MultiCpu ["-t","Cpu:<total0> <total1>","-L","10","-H","50","--normal","#429942","--high","#A36666"] 10
	   				, Run Memory ["-t", "Mem:<usedratio>%"] 10
	   				, Run Date "%H:%M" "date" 10
					, Run Com "sh" ["~/dev/xmobar/temperature"] "temp" 100
					, Run Com "sh" ["~/dev/xmobar/netstats"] "netstat" 100
					, Run Com "sh" ["~/dev/xmobar/todo"] "todo" 500
					, Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{[ %todo% ][ %temp% <fc=#7965ac>-</fc> %netstat% <fc=#7965ac>-</fc> %eth0% <fc=#7965ac>-</fc> %multicpu% <fc=#7965ac>-</fc> %memory% <fc=#7965ac>-</fc> %date% ]"
	   }
